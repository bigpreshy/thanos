{
  
  "manifest_version": 2,
  "name": "Thanos",
  "version": "0.1",
  "description": "Snap off everything",
  
  
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
       
      "js": ["content.js"]
    }
  ], 
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "thanos.png"
  }
    
}
